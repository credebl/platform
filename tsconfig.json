      "@credebl/user-request": [
        "libs/user-request/src"
      ],
      "@credebl/user-request/*": [
        "libs/user-request/src/*"
      ],
      "credebl/utility": [
        "libs/utility/src"
      ],
      "credebl/utility/*": [
        "libs/utility/src/*"
      ]
    },
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]

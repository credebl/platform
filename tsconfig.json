{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "paths": {
      "@credebl/aws": [
        "libs/aws/src"
      ],
      "@credebl/aws/*": [
        "libs/aws/src/*"
      ],
      "@credebl/client-registration": [
        "libs/client-registration/src"
      ],
      "@credebl/client-registration/*": [
        "libs/client-registration/src/*"
      ],
      "@credebl/common": [
        "libs/common/src"
      ],
      "@credebl/common/*": [
        "libs/common/src/*"

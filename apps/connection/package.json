{
  "name": "@credebl/connection-service",
  "version": "2.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.app.json",
    "generate": "npx prisma generate --schema=node_modules/@credebl/prisma-service/prisma/schema.prisma",
    "cleanup": "rm -rf node_modules dist pnpm-lock.yaml && rm -rf ~/.cache/prisma ~/.pnpm-store",
    "start": "ts-node src/main.ts"
  },
  "dependencies": {
    "@credebl/common": "file:../../credebl-common-2.0.0.tgz",
    "@credebl/logger": "file:../../credebl-logger-2.0.0.tgz",
    "@credebl/prisma-service": "file:../../credebl-prisma-service-2.0.0.tgz",
    "@credebl/user-management": "file:../../credebl-user-management-2.0.0.tgz",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/microservices": "^10.4.20",
    "cache-manager": "^5.7.6",
    "nats": "^2.29.3",
    "rxjs": "^7.8.2"
  }
}
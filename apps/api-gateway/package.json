        "@credebl/prisma-service": "file:../../credebl-prisma-service-2.0.0.tgz",
        "@credebl/user-activity": "file:../../credebl-user-activity-2.0.0.tgz",
        "@credebl/user-request": "file:../../credebl-user-request-2.0.0.tgz",
        "@credebl/aws": "file:../../credebl-aws-2.0.0.tgz",
        "@nestjs/common": "^10.4.19",
        "@nestjs/core": "^10.4.19",
        "@nestjs/microservices": "^10.4.19",
        "nats": "^2.29.3"
